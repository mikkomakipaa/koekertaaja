# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_placeholder
SUPABASE_SERVICE_ROLE_KEY=your_supabase_srv_key_placeholder

# AI providers (set at least one API key)
ANTHROPIC_API_KEY=your_anthropic_api_key_placeholder
OPENAI_API_KEY=your_openai_api_key_placeholder

# Provider toggle (optional)
AI_ENABLE_OPENAI=false

# Public app URL (optional)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# CORS allowlist for credentialed browser API requests
# Comma-separated exact origins only (no wildcards)
CORS_ALLOWED_ORIGINS=http://localhost:3000,https://your-app.example.com

# Admin access (comma-separated list)
ADMIN_EMAILS=admin@example.com
