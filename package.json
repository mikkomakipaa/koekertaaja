{
  "name": "exam-prepper",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "secrets:scan": "sh -c 'if rg -n \"(\\bsk-[A-Za-z0-9]{16,}|SUPABASE_SERVICE_ROLE_KEY=eyJ|ANTHROPIC_API_KEY=sk-|OPENAI_API_KEY=sk-)\" .env.example README.md docs/SECURITY_REVIEW.md --hidden; then echo \"Potential secret pattern detected.\"; exit 1; else echo \"No secret patterns found in sample/doc files.\"; fi'",
    "test": "node --test --loader ./scripts/ts-node-loader.mjs \"tests/**/*.test.ts\"",
    "analyze": "ANALYZE=true npm run build"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.30.1",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/katex": "^0.16.7",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "file-type": "^21.0.0",
    "katex": "^0.16.25",
    "next": "^16.1.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2",
    "react": "19.0.1",
    "react-dom": "19.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
